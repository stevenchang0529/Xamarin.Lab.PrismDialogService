<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Xamarin.Lab.PrismDialogService.Views.MainPage"
             Title="{Binding Title}">

    <StackLayout HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
        <Button Text="ShowAlert" Command="{Binding OnShowAlert}" />
        <Button Text="ShowConfirm" Command="{Binding OnShowConfirm}" />
        <Button Text="ShowSheet" Command="{Binding OnShowSheet}" />
        <Button Text="MyAlert" Command="{Binding OnMyAlert}" />
    </StackLayout>

</ContentPage>